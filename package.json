{
  "name": "ahwr-backoffice-ui",
  "version": "0.0.1",
  "description": "Back office UI for AHWR service",
  "homepage": "https://github.com/DEFRA/ahwr-backoffice-ui",
  "main": "app/index.js",
  "type": "module",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-push-hook": "npm run format:check && npm run lint && npm test",
    "postinstall": "npm run setup:husky",
    "pretest": "npm run lint",
    "test": "jest --runInBand",
    "test:watch": "jest --coverage=false --onlyChanged --watch --runInBand",
    "test:watch:all": "npm run test:watch & npm run build:watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js --coverage=false --onlyChanged --watch --runInBand --no-cache",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start:watch": "npm run build:watch & npm run start:nodemon",
    "start:debug": "nodemon --inspect-brk=0.0.0.0 --ext css,js,njk --legacy-watch app/index.js",
    "start:nodemon": "nodemon --inspect=0.0.0.0 --ext css,js,njk --legacy-watch app/index.js"
  },
  "author": "Defra",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@azure/msal-node": "3.8.0",
    "@defra/hapi-tracing": "1.22.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "9.1.4",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/cookie": "11.0.2",
    "@hapi/crumb": "8.0.1",
    "@hapi/hapi": "21.4.3",
    "@hapi/inert": "6.0.5",
    "@hapi/vision": "6.1.0",
    "@hapi/wreck": "18.1.0",
    "@hapi/yar": "10.1.1",
    "@ministryofjustice/frontend": "2.2.5",
    "aws-embedded-metrics": "4.2.0",
    "ffc-ahwr-common-library": "3.4.1",
    "global-agent": "3.0.0",
    "govuk-frontend": "5.10.0",
    "hapi-pino": "12.1.0",
    "http-status-codes": "2.3.0",
    "ioredis": "5.6.1",
    "joi": "17.13.3",
    "jquery": "3.7.1",
    "nunjucks": "3.2.4",
    "pino": "10.1.0",
    "undici": "7.10.0",
    "xml2js": "0.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "babel-jest": "30.0.0",
    "cheerio": "1.0.0",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.11.0",
    "dotenv": "16.4.7",
    "eslint-config-prettier": "10.1.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.3",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jest-junit": "13.2.0",
    "jest-when": "3.7.0",
    "mini-css-extract-plugin": "2.9.2",
    "neostandard": "0.12.1",
    "nodemon": "3.1.9",
    "pino-pretty": "8.1.0",
    "prettier": "3.5.3",
    "resolve-url-loader": "5.0.0",
    "sass": "1.83.1",
    "sass-loader": "12.6.0",
    "webpack": "5.97.1",
    "webpack-cli": "4.10.0"
  },
  "overrides": {
    "@azure/msal-node": {
      "jsonwebtoken": "9.0.2"
    },
    "brace-expansion": "2.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test-output/**"
    ]
  }
}
